<div class="user-form">
  <h1 class="user-form__title"><%= t(:developer_registration_head_text) %></h1>
  <%= form_tag(profile_path, method: :patch) do %>

    <div class="setup-info">
      <% @all_skills.each do |key, value| %>
        <%= check_box_tag 'skills[]', value %>
        <%= label_tag(key, value) %>
      <% end %>
    </div>

    <div class="form_button">
      <%= submit_tag t(:next_button), class: "button button--blue button--large user-form__button"%>
    </div>
  <% end %>
</div>
